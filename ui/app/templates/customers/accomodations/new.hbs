<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Add Accomodation</h2>
        <ol class="breadcrumb">
            <li>
              {{#link-to "customers.dashboard"}}Dashboard{{/link-to}}
            </li>
            <li>
              <a {{ action "cancel" }}> Accomodations </a>
            </li>
            <li class="active">
              {{#link-to "customers.accomodations.new"}}<strong>Add</strong>{{/link-to}}
            </li>
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-xs-12 col-sm-offset-3 col-sm-6">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>Provide your new Accomodation details</h5>
                </div>
                <div class="ibox-content">
                  {{customers.accomodations.accomodation-crud-form model=model }}
                </div>
            </div>
        </div>
    </div>
</div>

{{outlet}}