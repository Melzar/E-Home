            <div class="ibox">
                <div class="ibox-title">
                    <h5>Provide your Control details and type</h5>
                </div>
                <div class="ibox-content">
                  {{customers.controls.control-crud-form model=model}}
                </div>
            </div>
        </div>
    </div>
</div>

