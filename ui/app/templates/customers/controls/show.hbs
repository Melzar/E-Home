<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Controls</h2>
        <ol class="breadcrumb">
            <li>
              {{#link-to "customers.dashboard"}}Dashboard{{/link-to}}
            </li>
            <li>
              {{#link-to "customers.controls"}}Controls{{/link-to}}
            </li>
            <li class="active">
                <strong>{{model.name}}</strong>
            </li>
        </ol>
    </div>
</div>

<div class="p-w-md m-t-sm">
    <div class="row text-center m-b-lg">
        <div class="col-xs-12">
            <div class="widget yellw-bg no-padding">
                <div class="p-m">
                    <h1 class="m-xs">$ 50,992</h1>

                    <h3 class="font-bold no-margins">
                        Half-year revenue margin
                    </h3>
                    <small>Sales marketing.</small>
                </div>
                <div class="flot-chart">
                    <div flot="" class="flot-chart-content ng-isolate-scope" dataset="chart.flotChartData3"
                         options="chart.flotChartOptions3">
                        <div style="width: 100%; height: 100%; padding: 0px; position: relative;">
                            <canvas class="flot-base"
                                    style="direction: ltr; position: absolute; left: 0px; top: 0px; width: 385px; height: 100px;"
                                    width="770" height="200"></canvas>
                            <canvas class="flot-overlay"
                                    style="direction: ltr; position: absolute; left: 0px; top: 0px; width: 385px; height: 100px;"
                                    width="770" height="200"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

{{outlet}}
